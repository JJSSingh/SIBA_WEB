@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>
<p>Use this area to provide additional information.</p>

<h3>SubirArchivo</h3>
<form asp-action="UploadFile" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="file" class="control-label"></label>
        <input type="file" name="file" />
    </div>
    <div class="form-group">
        <input type="submit" value="subir" class="btn btn-default" />
    </div>
</form>

<h3>SubirArchivo2</h3>
<form asp-action="UploadFile2" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="file" class="control-label"></label>
        <input type="file" name="files" multiple/>
    </div>
    <div class="form-group">
        <input type="submit" value="subir" class="btn btn-default" />
    </div>
</form>



